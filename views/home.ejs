<%- include("partials/header",{id:id ? id:null, record: record ? record : null}) %>

<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.1.1/socket.io.js"></script>
<img id="image">
<script>
    const socket = io.connect('http://localhost:1025');
    socket.on('image',image => {
        const imageElement = document.getElementById('image');
        imageElement.src = `data:image/jpeg;base64,${image}`  ;
    })
</script>


<div class="container" style="text-align: center; width: 100%; margin: auto;">
    <img src="/images/Sample.jpg" alt="Reserve Bank of India" style="margin: 12% auto; text-align: center; width: 70%;">
</div>


